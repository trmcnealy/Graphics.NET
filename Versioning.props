<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <PACKAGE_VERSION>0.0.1</PACKAGE_VERSION>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros">
    <GRAPHICS_VERSION>$(PACKAGE_VERSION)</GRAPHICS_VERSION>
    <RUNTIME_GRAPHICS_VERSION>$(PACKAGE_VERSION)</RUNTIME_GRAPHICS_VERSION>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="GRAPHICS_VERSION">
      <Value>$(GRAPHICS_VERSION)</Value>
    </BuildMacro>
    <BuildMacro Include="RUNTIME_GRAPHICS_VERSION">
      <Value>$(RUNTIME_GRAPHICS_VERSION)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>